# -*- MakeFile -*-

NAME = holberton
CC = gcc
SRC = main.c holberton.c
OBJ = $(SRC:.c=.o)
CFLAGS = -Wall -Werror -Wextra -pedantic
.PHONY : all clean oclean fclean re

all: $(NAME)

clean:
	$(RM) *~ $(NAME)

oclean:
	$(RM) $(OBJ)

fclean: clean oclean

re: oclean $(NAME)

$(NAME): m.h $(OBJ)
	$(CC) -Wall -Werror -Wextra -pedantic $(OBJ) -o $(NAME)
